BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "Conta" (
	"CPF"	TEXT,
	"nome"	TEXT,
	"senha"	TEXT,
	PRIMARY KEY("CPF")
);
CREATE TABLE IF NOT EXISTS "Pagamento" (
	"codigo"	TEXT,
	"data"	TEXT,
	"percentual"	TEXT,
	"estado"	TEXT,
	"titulo"	TEXT,
	FOREIGN KEY("titulo") REFERENCES "Titulo"("codigo"),
	PRIMARY KEY("codigo")
);
CREATE TABLE IF NOT EXISTS "Titulo" (
	"codigo"	TEXT,
	"emissor"	TEXT,
	"setor"	TEXT,
	"emissao"	TEXT,
	"vencimento"	TEXT,
	"valor"	TEXT,
	"conta"	INTEGER,
	FOREIGN KEY("conta") REFERENCES "Conta"("CPF"),
	PRIMARY KEY("codigo")
);
